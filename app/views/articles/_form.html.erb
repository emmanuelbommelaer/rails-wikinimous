<%= form_with model: @article do |a| %>
<div>
  <div class="d-flex justify-content-between">
    <%= a.label :title %>
    <%= a.text_field :title %>
  </div>
  <div class="d-flex justify-content-between">
    <%= a.label :content %>
    <%= a.text_field :content%>
  </div>
</div>
  <p><%= a.submit %></p>
<% end %>
